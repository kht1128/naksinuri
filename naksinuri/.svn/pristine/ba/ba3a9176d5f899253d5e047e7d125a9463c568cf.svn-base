html,body{margin:0;padding:0;height:100%;}
body{font:14px/1.2 'Lato','Malgun Gothic','Nanum Gothic','Nanum Myeongjo','돋움', Dotum, '굴림', Gulim, arial, sans-serif;color:#333;background-color:#ececf1}
dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,textarea,p,blockquote,tbody,tfoot,thead,th,td,article,figure{margin:0;padding:0;}
input,textarea,select,td,th,button{color:#444;vertical-align:middle;}
h1,h2,h3,h4,h5,h6{font-weight:normal;font-size:100%;}
table{width:100%;border-spacing:0;border-collapse:collapse;table-layout:fixed;}
ol,ul,li,dl{list-style:none;}
hr, legend, caption, .skip{position:absolute;left:-5000px;top:0;overflow:hidden;line-height:0.1%;font-size:0.1%;width:0.1%;height:0.1%;}
caption{position:relative;}
img,fieldset{border:0 none;}
label,button{cursor:pointer;}
em,address{font-style:normal;}
a:link,a:visited{text-decoration:none;}
a:hover,a:active,a:focus{text-decoration:none;}
img{max-width:100%}

input[type=text],input[type=password],input[type=submit],textarea{-webkit-border-radius:0;-webkit-appearance:none;}
input[type=text]::-webkit-input-placeholder{color:#b4b4b4}
input[type=text]:-moz-placeholder{color:#b4b4b4}
input[type=text]::-moz-placeholder{color:#b4b4b4}
input[type=text]:-ms-input-placeholder{color:#b4b4b4}
input[type=password]::-webkit-input-placeholder{color:#b4b4b4}
input[type=password]:-moz-placeholder{color:#b4b4b4}
input[type=password]::-moz-placeholder{color:#b4b4b4}
input[type=password]:-ms-input-placeholder{color:#b4b4b4}

.floats{zoom:1}
.floats:after{content:'';display:block;clear:both}
.floats > li{float:left}

.frm_input,
.frm_textarea{max-width:90%;padding:8px;margin:2px 0;border:1px solid #e5e5e5;-moz-border-radius:1px;-webkit-border-radius:1px;border-radius:1px;}
.frm_input:focus,
.frm_textarea:focus{border-color:#ccc}
.frm_select{padding:7px;margin:2px 0;border:1px solid #e5e5e5;-moz-border-radius:1px;-webkit-border-radius:1px;border-radius:1px;min-height:36px;}
.readonly{background-color:#f7f7f7;border:1px solid #bbb}

.btn_size1{display:inline-block;height:30px;line-height:30px;padding:0 10px;margin:0 2px;text-align:center;font-weight:bold;font-size:14px;vertical-align:middle !important}
.btn_size2{display:inline-block;padding:15px 50px;margin:0 3px;text-align:center;font-weight:bold;font-size:14px;vertical-align:top;cursor:pointer}
.btn_size3{display:inline-block;height:18px;line-height:18px;padding:0 10px;margin:0 2px;text-align:center;font-weight:bold;font-size:12px;}
.btn_violet{color:#fff;background-color:#263692;border:1px solid #263692}
.btn_gray{color:#404040;background-color:#fff;border:1px solid #ccc}
.btn_orange{color:#fff;background-color:#f9ac41;border:1px solid #f9ac41}
.btn_bordergray{color:#666;background-color:#fff;border:1px solid #263692}
.btn_white{line-height:39px;border:1px solid #ccc;background-color:#fff}
.btn_red{background-color:#ff3333;color:#fff}
.btn_blue{background-color:#266eb3;color:#fff}
.btn_orange2{background-color:#e96828;color:#fff}


.txt_red{color:#ff6c00;}
.font_set{font: 14px/1.2 'Malgun Gothic','돋움', Dotum, '굴림', Gulim, arial, sans-serif;}

.m20{margin-top:20px;}

input.btn_size1{height:32px;line-height:26px;cursor:pointer;vertical-align:top}

#wrapper{position:relative;min-width:960px;min-height:100%;background:url("../img/left_bg.gif") repeat-y left}

/* BCS Agent */
	#bcs-agent{position:relative;overflow:hidden;width:150px;-moz-border-radius:16px;-webkit-border-radius:16px;border-radius:16px;}
	#bcs-agent ul li a{
		display:block;width:50px;height:32px;line-height:32px;background-color:#666;color:#fff;text-align:center;font-weight:700;
		-webkit-transition: all 0.3s ease-in-out;-moz-transition: all 0.3s ease-in-out;-o-transition: all 0.3s ease-in-out;-ms-transition: all 0.3s ease-in-out;transition: all 0.3s ease-in-out;
		}
	#bcs-agent ul li a:hover{background-color:#555}
	#bcs-agent ul li a.active{background-color:#f9ac41;text-shadow:1px 1px 1px #e48424}

/* 로그인 페이지 */
	#login_area{position:relative;width:450px;height:480px;margin:100px auto;background-color:#fff;letter-spacing:0.2pt}

	#login_area .l_login{position:relative;width:270px;margin:0 auto;padding-top:52px}
	#login_area .l_login h1 a{display:block;text-align:center;color:#333;font-size:20px;font-weight:bold}
	#login_area .l_login h2{margin:50px 0 15px;font-size:18px;text-align:center}
	#login_area .l_login ul li{padding:5px 0}
	#login_area .l_login .frm_login{
		width:248px;height:42px;line-height:42px;padding:0 10px;font-weight:bold;letter-spacing:0.2pt;background-color:#f7f7f7;
		border-left:1px solid #d4d4d4;border-top:1px solid #b1b1b1;border-right:1px solid #eaeaea;border-bottom:1px solid #e4e4e4;
		-moz-border-radius:3px;-webkit-border-radius:3px;border-radius:3px;
		}
	#login_area .l_login .btn_login{
		width:270px;height:62px;line-height:62px;color:#fff;font-weight:700;border:none;background-color:#263692;text-align:center;font-size:15px;cursor:pointer;
		-moz-border-radius:3px;-webkit-border-radius:3px;border-radius:3px;
		}
	#login_area .l_login .version{margin-top:40px;font-size:11px;text-align:center}

	#login_area .r_link{float:right;position:relative;width:320px;padding-right:56px}
	#login_area .r_link #bcs-agent{position:absolute;top:20px;right:20px}
	#login_area .r_link .btn_box{margin-top:140px;}
	#login_area .r_link .btn_box li{width:160px;}
	#login_area .r_link .btn_box li a{
		display:block;width:160px;height:130px;padding-top:30px;text-align:center;color:#3d2172;font-weight:bold;
		-moz-border-radius:10px;-webkit-border-radius:10px;border-radius:10px;
		}
	#login_area .r_link .btn_box li a em{display:block;padding-top:8px;text-transform:uppercase}
	#login_area .r_link .btn_box li a:hover{
		width:158px;height:128px;background-color:#f2f2f2;
		border-left:1px solid #d4d4d4;border-top:1px solid #b1b1b1;border-right:1px solid #eaeaea;border-bottom:1px solid #e4e4e4;
		}
	#login_area .copyright{position:absolute;bottom:-30px;right:0px;font-size:11px;width:450px;text-align:center}

/* 상단 헤더 */
	#header{
		position:fixed;top:0;left:0;bottom:0;width:250px;height:100%;background:#4356c5;}
	#header h1 a{display:block;height:62px;line-height:62px;background-color:#2f2c2f;color:#fff;font-weight:bold;font-size:20px;text-align:center;text-transform:uppercase}

/* 좌측메뉴 */
	#gnb{position:relative;margin-top:20px;}
	#gnb ul li.sub-menu > a{
		display:block;height:42px;line-height:42px;padding:8px 15px 8px 27px;color:#c7deee;font-size:15px;font-weight:bold;
		-webkit-transition: all 0.3s ease-in-out;-moz-transition: all 0.3s ease-in-out;-o-transition: all 0.3s ease-in-out;-ms-transition: all 0.3s ease-in-out;transition: all 0.3s ease-in-out;
		}
	#gnb ul li.sub-menu > a span{display:inline-block;font-size:24px;vertical-align:middle}
	#gnb ul li.sub-menu > a em{display:inline-block;margin-left:10px;vertical-align:middle}
	#gnb ul li.sub-menu > a:hover{color:#fff;font-weight:bold;background-color:#263692}
	#gnb ul li.sub-menu > a:hover span{color:#fff}
	#gnb ul li.sub-menu.active > a{color:#fff;font-weight:bold;background-color:#263692}
	#gnb ul li.sub-menu.active > a span{color:#fff}
	#gnb ul li ul{display:none;padding:10px 0;background-color:#3c4dac;}
	#gnb ul li ul li a{
		display:block;height:40px;line-height:40px;padding-left:60px;color:#fff;font-weight:bold;
		-webkit-transition: all 0.3s ease-in-out;-moz-transition: all 0.3s ease-in-out;-o-transition: all 0.3s ease-in-out;-ms-transition: all 0.3s ease-in-out;transition: all 0.3s ease-in-out;
	}
	#gnb ul li ul li a.active,
	#gnb ul li ul li a:hover{background-color:#251826;font-weight:bold;color:#f9ac41}
	#gnb ul li.active ul{display:block}

/* 상단 메뉴 */
	#top-mnu{position:relative;height:62px;padding:0 30px;background-color:#fff;-moz-box-shadow:0px 0px 7px rgba(0,0,0,0.2);-webkit-box-shadow:0px 0px 7px rgba(0,0,0,0.2);box-shadow:0px 0px 7px rgba(0,0,0,0.2);}
	#top-mnu .admin_set_in{position:absolute;top:15px;left:280px;font-size:13px;}
	#top-mnu .admin_set_in li{margin-left:5px;line-height:32px}
	#top-mnu .admin_set_in li:first-child{padding-right:5px;}
	#top-mnu .admin_set_in li.pagemove a{display:block;height:30px;line-height:28px;padding:0 10px;background-color:#4356c5;border-radius:5px;color:#fff}
	#top-mnu .admin_set_out{position:absolute;top:15px;right:30px;font-size:13px;}
	#top-mnu .admin_set_out li{margin-left:5px;line-height:32px}
	#top-mnu .admin_set_out li:first-child{padding-right:5px;}
	#top-mnu .admin_set_out li.wastebasket{padding-top:4px}
	#top-mnu .admin_set_out li.wastebasket a{display:block;height:24px;line-height:22px;padding:0 10px;background-color:#cc0000;border-radius:5px;color:#fff}
	#top-mnu .admin_set_out li.logout a{
		display:block;width:32px;height:32px;;background-color:#666;color:#fff;text-align:center;
		-moz-border-radius:50%;-webkit-border-radius:50%;border-radius:50%;
		-webkit-transition: all 0.3s ease-in-out;-moz-transition: all 0.3s ease-in-out;-o-transition: all 0.3s ease-in-out;-ms-transition: all 0.3s ease-in-out;transition: all 0.3s ease-in-out;
		}
	#top-mnu .admin_set_out li.logout a:hover{background-color:#f9ac41}

/* 중간콘텐츠 */
	#container{position:relative;margin-left:250px;padding-bottom:50px;}
	#container h2{font-size:16px;padding-left:7px;margin-bottom:15px;border-left:5px solid #263692;font-weight:bold}
	#container h1.bigTit{
		position:relative;height:60px;line-height:58px;padding:0px 20px;margin-bottom:20px;background-color:#fff;font-size:20px;font-weight:bold;letter-spacing:-.5pt;color:#555;
		-moz-box-shadow:0px 1px 5px rgba(0,0,0,0.1);-webkit-box-shadow:0px 1px 5px rgba(0,0,0,0.1);box-shadow:0px 1px 5px rgba(0,0,0,0.1);}
	#container h1.bigTit .trash_box{position:absolute;top:0px;right:0px;height:60px;line-height:56px;padding:0 20px;}
	#container h1.bigTit .trash_box select{height:36px;padding:0 0 0 10px;margin:0;border:1px solid #dcdddd;background-color:/*#666*/#c54343;color:#fff}
	#content{padding:20px;}
	.msg{margin-bottom:10px;padding:18px 20px;border:1px solid #c9c9c9;background-color:#e8e8e8}
	.plusminus{display:inline-block;width:17px;vertical-align:middle}
	.plusminus a{display:block;width:15px;height:13px;line-height:13px;margin:2px 0;border:1px solid #e5e5e5;color:#afafaf;font-size:11px;;text-align:center;font-weight:bold}
	.btn_area{margin:30px 0;}
	.btn_section_area{position:absolute;top:0;right:0;}
	.textcenter{text-align:center}
	.textright{text-align:right}
	.btn_area.textright{margin:10px 0 10px}
	.division{position:relative;margin-top:30px}
	.division-left{float:left}
	.division-right{float:right}
	#section-box{position:relative;margin-bottom:20px;}

	/* Dashboard */
		#dashboard{}
		#dashboard > ul > li{overflow:hidden;width:500px;margin:10px;background-color:#fff;-moz-border-radius:3px;-webkit-border-radius:3px;border-radius:3px;}
		#dashboard > ul > li h3{position:relative;height:40px;line-height:40px;padding:0 20px;border-bottom:1px solid #e5e5e5;color:#404040;font-weight:bold;font-size:16px}
		#dashboard > ul > li h3 a{position:absolute;top:0;right:0;display:block;width:40px;height:40px;border-left:1px solid #e5e5e5;text-align:center;color:#999}
		#dashboard > ul > li h3 a:hover{background-color:#fafafa;color:#404040}
		#dashboard > ul > li.chart.cx2{width:1020px}
		#dashboard > ul > li.chart .ct{display:block;height:345px;background-color:#fff}
		#dashboard > ul > li.latest{background-color:#fff}
		#dashboard > ul > li.latest ul{padding:20px}
		#dashboard > ul > li.latest ul li{line-height:26px;font-size:13px}
		#dashboard > ul > li.latest ul li a{float:left;color:#333}
		#dashboard > ul > li.latest ul li a:hover{text-decoration:underline}
		#dashboard > ul > li.latest ul li span{float:right;font-size:12px;color:#999}
	
	/* Table */
		.table-box{position:relative;padding:1px;background-color:#fff;-moz-box-shadow:0px 1px 5px rgba(0,0,0,0.1);-webkit-box-shadow:0px 1px 5px rgba(0,0,0,0.1);box-shadow:0px 1px 5px rgba(0,0,0,0.1);}
		.table-box h3{height:40px;line-height:40px;padding:0 20px;border-bottom:1px solid #e5e5e5;font-weight:bold}
		.padding_box{padding:0 20px}
		
		/* 테이블 글쓰기 */
		.t_write{}
		.t_write tbody tr th,
		.t_write tbody tr td{min-height:32px;padding:14px 10px;border-bottom:1px dashed #e5e5e5}
		.t_write tbody tr:last-child th,
		.t_write tbody tr:last-child td{border-bottom:none}
		.t_write tbody tr th{font-weight:normal;text-align:right}
		.t_write tbody tr th.txt-left{text-align:left;}
		.t_write tbody tr th.txt-center{text-align:center;}
		.t_write tbody tr th.txt-right{text-align:right;}
		.t_write tbody tr th h3._title{padding:10px 0px;}

		#table-list{margin-top:20px;}
		#tabarea{position:relative;margin-bottom:20px;}
		#tabarea .trash_box{position:absolute;top:0;right:0;}
		#tabarea ul{border-bottom:1px solid #ccc}
		#tabarea ul li{width:20%;margin-left:-1px;margin-bottom:-1px}
		#tabarea ul.tab6 li{width:16.6666%}
		#tabarea ul li:first-child{margin-left:0}
		#tabarea ul li a{display:block;height:36px;line-height:36px;border:1px solid #ccc;text-align:center;background-color:#f7f7f7;color:#333}
		#tabarea ul li a.on{background-color:#ececf1;font-weight:bold;border-bottom:1px solid #ececf1}

		#search_form{
			padding:15px;margin-bottom:20px;background-color:#fff;
			-moz-box-shadow:0px 1px 5px rgba(0,0,0,0.1);-webkit-box-shadow:0px 1px 5px rgba(0,0,0,0.1);box-shadow:0px 1px 5px rgba(0,0,0,0.1);
			}
		#search_form .t_search tr th,
		#search_form .t_search tr td{padding:5px 10px;min-height:32px;border-top:1px dashed #ddd;font-weight:normal}
		#search_form .t_search tr:first-child th,
		#search_form .t_search tr:first-child td{border-top:none}
		#search_form .t_search .frm_input,
		#search_form .t_search .frm_textarea,
		#search_form .t_search .frm_select{background-color:#fff}
		
		/* 테이블 리스트 */
		.t_list_area .listtop{margin-bottom:10px;line-height:30px;font-size:13px;}
		.t_list_area .listtop .totalnum{float:left;margin-left:10px;}
		.t_list_area .listtop .listnum{float:right;margin-right:10px;}
		.t_list{margin-bottom:20px;font-size:13px;background-color:#fff}
		.t_list thead tr th{padding:8px 0;border-left:1px solid #fff;background-color:#4356c5;color:#fff}
		.t_list thead tr th:first-child{border-left:none}
		.t_list tbody tr td{padding:8px 5px;border-bottom:1px dashed #dedede}
		.t_list tbody tr:last-child td{border-bottom:1px solid #b4b4b4}
		.t_list tbody tr td a{color:#333}
		.t_list tbody tr td a:hover{text-decoration:underline}
		.t_list tbody tr td a.txt_red{color:#ff0000}
		.t_list tbody tr td .stat{display:inline-block;width:80px;padding:3px 0;border:1px solid #ccc;background-color:#f7f7f7}
		.t_list tbody tr td .stat.stat_green{background-color:#dff0d8;color:#3c763d;border-color:#c4d7b4}
		.t_list tbody tr td .stat.stat_blue{background-color:#d9edf7;color:#31708f;border-color:#9fcdd6}
		.t_list tbody tr td .stat.stat_red{background-color:#f2dede;color:#a94442;border-color:#d0afb4}
		
		/* 설문조사 */
		.survey_view{padding:30px 0}
		.survey_view h4{height:40px;line-height:40px;padding:0 20px;background-color:#666;font-size:20px;font-weight:bold;color:#fff}
		.survey_view h4 span.date{font-size:16px}
		.survey_view .suveybox{padding:20px;}
		.survey_view .suveybox dl{padding-top:20px;margin-top:20px;border-top:1px dashed #dcdddd}
		.survey_view .suveybox dl:first-child{padding-top:0;margin-top:0px;border-top:0}
		.survey_view .suveybox dl dt{display:table;margin-bottom:10px;font-size:17px;}
		.survey_view .suveybox dl dt em.num{display:table-cell;width:40px;font-size:22px;}
		.survey_view .suveybox dl dt span.subject{display:table-cell;font-weight:bold}
		.survey_view .suveybox dl dt span.subject span.type{font-size:16px;font-weight:normal;color:#888}
		.survey_view .suveybox dl dd{padding-left:40px;color:#666}
		.survey_view .suveybox dl dd .btns{display:inline-block;height:26px;line-height:26px;padding:0 10px;background-color:#0099cc;border-radius:3px;color:#fff;font-size:14px;font-weight:normal}
		.survey_view .suveybox dl dd .subjective{border:1px solid #e0e0e0}
		.survey_view .suveybox dl dd .subjective .subjective_con{padding:5px;border-top:1px dashed #dcdddd}
		.survey_view .suveybox dl dd .subjective .subjective_con:first-child{border-top:none}
		.survey_view .suveybox dl dd .subjective .subjective_con:nth-child(2n){background-color:#fafafa}
		.survey_view .suveybox dl dd ul.multiple > li{margin-top:10px}
		.survey_view .suveybox dl dd ul.multiple > li .multiple_subject{font-weight:bold;font-size:15px}
		.survey_view .suveybox dl dd ul.multiple > li .progress{position:relative;overflow:hidden;height:20px;background-color:#f7f7f7;margin-top:5px;border-radius:10px;}
		.survey_view .suveybox dl dd ul.multiple > li .progress .prograss-bar{height:20px;line-height:20px;background-color:#66cccc}
		.survey_view .suveybox dl dd ul.multiple > li .progress .percent{position:absolute;top:0px;right:20px;line-height:20px;font-weight:bold}
		.survey_view .suveybox dl dd ul.multiple > li .subjective{margin-left:30px;margin-top:5px}
		
		/* 댓글 박스 */
		.reply_box{position:relative;padding:30px;margin:30px -20px 0;border-top:1px dashed #bdbdbd;background-color:#f7f7f7}
		.reply_box .reply_list{}
		.reply_box .reply_list .reply_con{
			padding:20px;margin-top:10px;background-color:#fff;
			-moz-box-shadow:0px 1px 10px rgba(0,0,0,0.2);-webkit-box-shadow:0px 1px 10px rgba(0,0,0,0.2);box-shadow:0px 1px 10px rgba(0,0,0,0.2);}
		.reply_box .reply_list .reply_con:first-child{margin-top:0;}
		.reply_box .reply_list .reply_con span.name{font-weight:bold;font-size:15px}
		.reply_box .reply_list .reply_con span.date{display:inline-block;margin:0 10px;font-size:13px;color:#999}
		.reply_box .reply_list .reply_con a.replyBtn{color:#666}
		.reply_box .reply_list .reply_con p{line-height:20px;margin-top:10px;color:#777;font-size:13px}

		/* 페이지 */
		#pagenation{margin:30px 0;text-align:center}
		#pagenation strong{
		    display: inline-block;
		    min-width: 26px;
		    height: 26px;
		    line-height: 26px;
		    margin: 0 2px;
		    border: 3px solid #ccc;
		    text-align: center;
		    color: #fff;
		    font-size: 13px;
		    background-color: #000;
		    -moz-border-radius: 3px;
		    -webkit-border-radius: 3px;
		    border-radius: 3px;
		    -moz-box-shadow: 0px 1px 2px rgba(0,0,0,0.1);
		    -webkit-box-shadow: 0px 1px 2px rgba(0,0,0,0.1);
		    box-shadow: 0px 1px 2px rgba(0,0,0,0.1);
		}
