<script src="/naksinuri_original/common_main/js/jquery.sticky.js"></script>
<script type="text/javascript">
$(document).ready(function(){
	/* 낚시산업정보 상세페이지 기본정보 스크롤 (Feat.sticky-kit) */
	$("#termsOfObedience #locabox").stick_in_parent()
	.on("sticky_kit:stick", function(e) {
		$('#locabox dl').addClass("scrollOn");
	})
	.on("sticky_kit:unstick", function(e) {
		$('#locabox dl').removeClass("scrollOn");
	});
});


//1024px 이하에서 컨텐츠 토글
if (matchMedia("screen and (max-width: 1024px)").matches) {
	$('.obedience_box h3 a').on('click',function(event) {
	    let $this = $(this);
	    
	    $this.parent().parent().siblings('.active').find('.obedience_con').stop().slideUp(500); 
	    $this.parent().parent().siblings('.active').removeClass('active');

	    if ( $this.parent().parent().hasClass('active') ) {
	    	$this.parent().parent().find('.obedience_con').stop().slideUp(500); 
	    	$this.parent().parent().removeClass('active');
	    }
	    else {
	    	$this.parent().parent().find('.obedience_con').stop().slideDown(500); 
	    	$this.parent().parent().addClass('active');
	    }
	});

	$('.obedience_con').on('click',function(event) {
	    event.stopPropagation();
	});
} 



	
</script>

