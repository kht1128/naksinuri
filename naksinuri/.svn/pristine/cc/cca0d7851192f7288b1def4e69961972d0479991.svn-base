.shortcut_list{position: absolute;bottom: 3%; left: 0; z-index: 50;width: 100%;;}
.shortcut_list ul{display: flex; justify-content: center; align-items: center; width: 35%;margin: 0 auto;}
.shortcut_list ul li{width: 33.33%;text-align: center; background-color: rgba(255, 255, 255, 0.8);position: relative;}
.shortcut_list ul li::after{content: "";width: 2px;height: 50%;position: absolute; right: 0;bottom: 12px; background-color: rgba(129, 129, 129, 0.3);}
.shortcut_list ul li:first-of-type{border-radius: 10px 0 0 10px;}
.shortcut_list ul li:last-of-type{border-radius: 0 10px 10px 0;}
.shortcut_list ul li a{display:block;width: 100%; padding: 15px;}
.shortcut_list ul li a span{position: relative;color: #333;padding-left: 30px;font-size: 1.6rem;font-weight: 500;}
.shortcut_list ul li a .ico1::after {content: "";background: url(../img/main/shortcut_ico_new_01.png) no-repeat center;position: absolute;top: 0; left: 0;width: 27px;height: 100%; background-size: contain;}
